
<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=EUC-KR">
	<title>music test</title>
	<link rel="stylesheet" href="/music/css/testcss.css">
</head>
	
<body>
	<div class="wrap">
		<header id="header">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="logo">
							<img src="/music/image/logo.jpg">
						</div>
					</div>
					<div class="col">
						<div class="row">
							<div class="login">
								<img src="/music/image/login.jpg">
							</div>
							<div class="search">
							 	<img src="/music/image/search.jpg">
							</div>
						</div>
					</div>
				</div>
			</div>
		</header>
		
		<nav id="navbar">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="menu">
							<ul>
								<li>메뉴1</li>
								<li>메뉴2</li>
								<li>메뉴3</li>
								<li>메뉴4</li>
								<li>메뉴5</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</nav>
		
		<section id="section1">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="title">
							검색결과
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col">
						<div class="searchMenu">
							<ul>
								<li>통합검색</li>
								<li>아티스트</li>
								<li>곡</li>
								<li>앨범</li>
								<li>뮤비</li>
								<li>가사</li>
								<li>컨텐츠</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</section>
		
		<section id="section2">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="artistList">	
							<div class="row">
								<div class="col">
									<div class="photo">
										<img src="">프로필 사진
									</div>
								</div>
								<div class="col">
									<div class="artistInfo">
										<ul>
											<li>이름</li>
											<li>유형</li>
											<li>연대</li>
											<li>데뷔</li>
										</ul>
									</div>
								</div>
							</div>
						</div>
					</div>
					<!-- col 늘리기 -->
				</div>
			</div>
		</section>
		
		<section id="section3">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="title">
							곡
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col">
						<div class="musicButton">
							<button>듣기</button>
							<button>전체 듣기</button>
							<button>담기</button>
							<button>다운로드</button>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col">
						<div class="musicList">
							<div class="row">
								<div class="col">
									번호
								</div>
								<div class="col">
									곡
								</div>
								<div class="col">
									아티스트
								</div>
								<div class="col">
									앨범명
								</div>
								<div class="col">
									<button>뮤비</button>
								</div>
								<div class="col">
									<button>담기</button>
								</div>
								<div class="col">
									<button>다운</button>
								</div>
							</div>
						<!-- row 늘리기 -->
						</div>
					</div>
				</div>
			</div>
		</section>
		
		<section id="section4">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="title">
							앨범
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col">
						<div class="album">
							<div class="row">
								<div class="col">
									<img src="">앨범커버
								</div>
								<div class="col">
									앨범제목<br>
									가수<br>
								</div>
							</div>
						</div>
					</div>
					<!-- col 늘리기 -->
				</div>
			</div>
		</section>
		
		<section id="section5">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="title">
							뮤직비디오
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col">
						<img src="">뮤비스틸컷<br>
						제목<br>
						가수
					</div>
					<!-- col 늘리기 -->
				</div>
			</div>
		</section>
		
		<section id="section6">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="title">
							가사
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col">
						<div class="verseList">
							<div class="row">
								<div class="col">
									<img src="">앨범커버
								</div>
								<div class="col">
									<div class="row">
										<div class="col">
											제목 아티스트
										</div>
										<div class="col">
											<button>듣기</button>
											<button>담기</button>
											<button>다운로드</button>
										</div>
									</div>
									<div class="row">
										<div class="col">
											가사 가사 가사 가사 가사 가사 가사 가사
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		
		<section id="section7">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="title">
							컨텐츠
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col">
						<div class="contents">
							<div class="row">
								<div class="col">
									<img src="">이미지
								</div>
								<div class="col">
									제목<br>
									내용<br>
								</div>
							</div>
						</div>
					</div>
					<!-- col 늘리기 -->
				</div>
			</div>
		</section>
		
		
		<footer id="footer">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="company">
							footer-company
						</div>
					</div>
				</div>
			</div>
		</footer>
	</div>
</body>
</html>